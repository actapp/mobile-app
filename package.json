{
  "name": "MySharePal",
  "version": "1.1.1",
  "jsVersion": "001",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "debug": "open -a 'React Native Debugger.app'",
    "debug-ios": "npm run debug && react-native run-ios",
    "test": "jest",
    "deploy": "node scripts/Deploy.js",
    "rr": "adb shell input text \"RR\"",
    "adbr": "adb reverse tcp:8081 tcp:8081",
    "adbc": "adb shell pm clear com.mysharepal",
    "release-android-sta": "appcenter codepush release-react -a mysharepal/ACT-Mobile -d Staging",
    "release-ios-sta": "appcenter codepush release-react -a mysharepal/ACT-Mobile-iOS -d Staging",
    "release-all-sta": "npm run release-android-sta && npm run release-ios-sta",
    "release-android-prod": "appcenter codepush release-react -a mysharepal/ACT-Mobile -d Production",
    "release-ios-prod": "appcenter codepush release-react -a mysharepal/ACT-Mobile-iOS -d Production",
    "release-all-prod": "npm run release-android-prod && npm run release-ios-prod",
    "rollback-ios-prod": "appcenter codepush rollback -a mysharepal/ACT-Mobile-iOS Production"
  },
  "dependencies": {
    "@microsoft/applicationinsights-react-native": "^1.0.0-rc5",
    "@microsoft/applicationinsights-web": "^2.0.0-rc5",
    "appcenter": "^1.12.2",
    "appcenter-analytics": "^1.12.2",
    "appcenter-crashes": "^1.12.2",
    "native-base": "^2.12.1",
    "react": "^16.8.6",
    "react-native": "^0.59.8",
    "react-native-animatable": "^1.3.2",
    "react-native-chart-kit": "^2.6.1",
    "react-native-cli": "^2.0.1",
    "react-native-code-push": "^5.7.0",
    "react-native-device-info": "^1.6.2",
    "react-native-firebase": "^5.2.3",
    "react-native-gesture-handler": "^1.0.17",
    "react-native-options-menu": "^2.0.2",
    "react-native-simple-store": "^1.3.0",
    "react-native-svg": "^9.4.0",
    "react-native-svg-charts": "^5.2.0",
    "react-native-ui-lib": "^3.20.1",
    "react-native-vector-icons": "^6.3.0",
    "react-navigation": "^3.4.1",
    "react-redux": "^7.0.3",
    "recharts": "^1.6.2",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "stacktrace-js": "^2.0.0",
    "victory-native": "^32.0.2"
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.3.1",
    "jest": "24.3.1",
    "metro-react-native-babel-preset": "0.53.0",
    "react-test-renderer": "16.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
